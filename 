[32m+[m[32m        this.sundaySubjectsLiveData.setValue(this.classroom.getTimeTable().getSundaySubjects());[m
[32m+[m[32m    }[m
[32m+[m[32m    public void addNewSubject(String subject, int day){[m
[32m+[m[32m        TimeTable timeTable = this.classroom.getTimeTable();[m
[32m+[m[32m        switch (day){[m
[32m+[m[32m            case 1:[m
[32m+[m[32m                timeTable.getMondaySubjects().add(subject);[m
[32m+[m[32m                break;[m
[32m+[m[32m            case 2:[m
[32m+[m[32m                timeTable.getTuesdaySubjects().add(subject);[m
[32m+[m[32m                break;[m
[32m+[m[32m            case 3:[m
[32m+[m[32m                timeTable.getWednesdaySubjects().add(subject);[m
[32m+[m[32m                break;[m
[32m+[m[32m            case 4:[m
[32m+[m[32m                timeTable.getThursdaySubjects().add(subject);[m
[32m+[m[32m                break;[m
[32m+[m[32m            case 5:[m
[32m+[m[32m                timeTable.getFridaySubjects().add(subject);[m
[32m+[m[32m                break;[m
[32m+[m[32m            case 6:[m
[32m+[m[32m                timeTable.getSaturdaySubjects().add(subject);[m
[32m+[m[32m                break;[m
[32m+[m[32m            case 7:[m
[32m+[m[32m                timeTable.getSundaySubjects().add(subject);[m
[32m+[m[32m                break;[m
[32m+[m[32m            default: snackbarText.setValue("Selecione um dia v√°lido...");[m
[32m+[m[32m            break;[m
[32m+[m[32m        }[m
[32m+[m[32m        HashMap<String, Object> classroomUpdates = new HashMap<>();[m
[32m+[m[32m        classroomUpdates.put("timeTable", timeTable);[m
[32m+[m[32m        new ClassroomDAO().updateClassroom(this.getInstitution().getId(), this.course.getId(), this.classroom.getId(), classroomUpdates);[m
[32m+[m[32m    }[m
[32m+[m[32m    public void removeSubject(int subjectIndex, int day){[m
[32m+[m[32m        TimeTable timeTable = this.getClassroom().getTimeTable();[m
[32m+[m[32m        switch (day){[m
[32m+[m[32m            case 1:[m
[32m+[m[32m                timeTable.getMondaySubjects().remove(subjectIndex);[m
[32m+[m[32m                break;[m
[32m+[m[32m            case 2:[m
[32m+[m[32m                timeTable.getTuesdaySubjects().remove(subjectIndex);[m
[32m+[m[32m                break;[m
[32m+[m[32m            case 3:[m
[32m+[m[32m                timeTable.getWednesdaySubjects().remove(subjectIndex);[m
[32m+[m[32m                break;[m
[32m+[m[32m            case 4:[m
[32m+[m[32m                timeTable.getThursdaySubjects().remove(subjectIndex);[m
[32m+[m[32m                break;[m
[32m+[m[32m            case 5:[m
[32m+[m[32m                timeTable.getFridaySubjects().remove(subjectIndex);[m
[32m+[m[32m                break;[m
[32m+[m[32m            case 6:[m
[32m+[m[32m                timeTable.getSaturdaySubjects().remove(subjectIndex);[m
[32m+[m[32m                break;[m
[32m+[m[32m            case 7:[m
[32m+[m[32m                timeTable.getSundaySubjects().remove(subjectIndex);[m
[32m+[m[32m                break;[m
[32m+[m[32m            default: snackbarText.setValue("Selecione um dia v√°lido...");[m
[32m+[m[32m                break;[m
[32m+[m[32m        }[m
[32m+[m[32m        HashMap<String, Object> classroomUpdates = new HashMap<>();[m
[32m+[m[32m        classroomUpdates.put("timeTable", timeTable);[m
[32m+[m[32m        new ClassroomDAO().updateClassroom(this.getInstitution().getId(), this.course.getId(), this.classroom.getId(), classroomUpdates);[m
[32m+[m[32m    }[m
[32m+[m[32m}[m
